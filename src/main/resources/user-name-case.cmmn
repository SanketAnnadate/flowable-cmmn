<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/CMMN/20151109/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:flowable="http://flowable.org/cmmn"
             xmlns:cmmndi="http://www.omg.org/spec/CMMN/20151109/CMMNDI"
             xmlns:dc="http://www.omg.org/spec/CMMN/20151109/DC"
             xmlns:di="http://www.omg.org/spec/CMMN/20151109/DI"
             targetNamespace="http://www.flowable.org/casedef">

  <case id="userNameInputCase" name="User Name Input Case">
    <casePlanModel id="casePlanModel1" name="User Enters Name" flowable:autoComplete="true">

        <planItem id="planItem1" definitionRef="taskEnterName" name="Enter Your Name">
          <entryCriterion id="entry1" sentryRef="sentry1"/>
        </planItem>
        <sentry id="sentry1"/>

      <humanTask id="taskEnterName" name="Enter Your Name Task" flowable:candidateGroups="users">
        <extensionElements>
          <flowable:formField id="userName" name="Your Name" type="string" required="true"/>
        </extensionElements>
      </humanTask>

    </casePlanModel>
  </case>
</definitions>
